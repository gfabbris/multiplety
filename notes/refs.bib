@article{Boseggia2013,
  author={S Boseggia and H C Walker and J Vale and R Springell and Z Feng and R S Perry and M Moretti Sala and H M RÃžnnow and S P Collins and D F
McMorrow},
  title={Locking of iridium magnetic moments to the correlated rotation of oxygen octahedra in {Sr${}_{2}$IrO${}_{4}$} revealed by x-ray resonant scattering},
  journal={J. Phys. Condens. Matter},
  volume={25},
  number={42},
  pages={422202},
  url={http://stacks.iop.org/0953-8984/25/i=42/a=422202},
  year={2013},
  abstract={Sr 2 IrO 4Â is a prototype of the class of Mott insulators in the strong spinâorbit interaction (SOI) limit described by a J effÂ =Â 1/2 ground state. In Sr 2 IrO 4 , the strong SOI is predicted to manifest itself in the locking of the canting of the magnetic moments to the correlated rotation by 11.8(1)Â°Â of the oxygen octahedra that characterizes its distorted layered perovskite structure. Using x-ray resonant scattering at the Ir L 3 edge we have measured accurately the intensities of Bragg peaks arising from different components of the magnetic structure. From a careful comparison of integrated intensities of peaks due to basal-plane antiferromagnetism, with those due to b -axis ferromagnetism, we deduce a canting of the magnetic moments of 12.2(8)Â°. We thus confirm that in Sr 2 IrO 4Â the magnetic moments rigidly follow the rotation of the oxygen octahedra, indicating that, even in the presence of significant non-cubic structural distortions, it is a close realization of the J effÂ =Â 1/2 state.}
}
	

