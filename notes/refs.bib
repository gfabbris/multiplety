@article{James2013,
  author={A J A James and R M Konik and K Huang and W-Q Chen and T M Rice and F C Zhang},
  title={Magnetism and Superconductivity in the Pseudogap Phase of Underdoped Cuprates},
  journal={Journal of Physics: Conference Series},
  volume={449},
  number={1},
  pages={012006},
  url={http://stacks.iop.org/1742-6596/449/i=1/a=012006},
  year={2013},
  abstract={The theoretical description of the anomalous properties of the pseudogap phase in the underdoped region of the cuprate phase diagram lags behind the progress in spectroscopic and other experiments. A phenomenological ansatz, based on analogies to the approach to Mott localization at weak coupling in lower dimensional systems, for the single particle propagator in the pseudogap phase has been proposed by Yang, Rice and Zhang. This ansatz has had success in describing a range of experiments, especially spectroscopies such as ARPES and aspects of STM results. Recently this approach has been extended to successfully interpret the magnetic excitations in the spin response response. In the charge channel, d-wave cooperon excitations accompany the opening of the pseudogap and they generate an additional d-wave attraction for quasiparticles at the remnant nodal Fermi surfaces.}
}
	

