@article{Dean2015,
title = "Insights into the high temperature superconducting cuprates from resonant inelastic X-ray scattering ",
journal = "Journal of Magnetism and Magnetic Materials ",
volume = "376",
number = "0",
pages = "3 - 13",
year = "2015",
issn = "0304-8853",
