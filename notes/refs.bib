@article{Brink2006,
  author={J. van den Brink and M. van Veenendaal},
  title={Correlation functions measured by indirect resonant inelastic X-ray scattering},
  journal={EPL (Europhysics Letters)},
  volume={73},
  number={1},
  pages={121},
  url={http://stacks.iop.org/0295-5075/73/i=1/a=121},
  year={2006},
  abstract={We express the cross-section for indirect resonant inelastic X-ray scattering in terms of an intrinsic dynamic correlation function of the system that is studied with this technique. We find that the cross-section is a linear combination of the charge response function and the dynamic longitudinal spin density correlation function of the electrons that couple to the core-hole. This result is asymptotically exact for both strong and weak local core-hole potentials. The relative charge and spin contribution to the inelastic spectral weight can be changed by varying the incident photon energy.}
}

@article{Brink2007,
  author={J. van den Brink},
  title={The theory of indirect resonant inelastic X-ray scattering on magnons},
  journal={EPL (Europhysics Letters)},
  volume={80},
  number={4},
  pages={47003},
  url={http://stacks.iop.org/0295-5075/80/i=4/a=47003},
  year={2007},
  abstract={Recent experiments reveal indirect resonant inelastic X-ray scattering (RIXS) to be a new probe of spin dynamics. Here I derive the cross-section for magnetic RIXS and determine the momentum-dependent four-spin correlation function that it measures. These results show that this technique offers information on spin dynamics that is complementary to, e.g. , neutron scattering. As an example the RIXS spectrum of Heisenberg antiferromagnets is calculated by considering a half filled single band Hubbard model at strong coupling and zero temperature. It turns out that only scattering processes that involve at least two magnons are allowed. Other selection rules imply that the scattering intensity vanishes for specific transferred momenta q , in particular for q =0. All results agree very well with recent experimental data.}
}
	

